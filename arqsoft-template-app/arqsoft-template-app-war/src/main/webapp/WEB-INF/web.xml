<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>appCore</display-name>

	<context-param>
		<param-name>webAppRootKey</param-name>
		<param-value>webapp.root.fnweb</param-value>
	</context-param>
	
	<resource-ref>
		<description>Datasource for FN DB</description>
		<res-ref-name>jdbc/jndiDataSource</res-ref-name>
<!-- 		<res-ref-name>jdbc/ds_oracle_fn_db</res-ref-name> -->
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
		<res-sharing-scope>Shareable</res-sharing-scope>
	</resource-ref>

	<welcome-file-list>
		<welcome-file>index.xhtml</welcome-file>
	</welcome-file-list>


	
</web-app>
