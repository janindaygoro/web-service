<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE generatorConfiguration
	PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
	"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="App" targetRuntime="MyBatis3">
		
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>
		
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin"/>
		
		<plugin type="org.mybatis.generator.plugins.RowBoundsPlugin" />
	
	
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost/app"
			userId="root"
			password="mysql" />
			
		<javaModelGenerator targetPackage="pe.com.aqsoft" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		
		<sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator targetPackage="pe.com.aqsoft.dao" 
			targetProject="src/main/java" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />	
		</javaClientGenerator>
		

		
		<table tableName="USER" domainObjectName="User"
			modelType="flat"
			enableDeleteByExample="true"
			enableUpdateByExample="true"
			selectByPrimaryKeyQueryId="false"
			selectByExampleQueryId="false">
			<columnOverride column="FLAG_ACTIVE" property="active" javaType="boolean" />
		</table>
				
		
	</context>
</generatorConfiguration>